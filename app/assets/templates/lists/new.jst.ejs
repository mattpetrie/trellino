<form id="add-list-form">
  <input type="hidden" name="list[board_id]" value="<%= board.id %>">
  <input type="hidden" name="list[rank]" value="1">
  <div class="input-group">
  <label for="list-title"></label>
    <input type="text" name="list[title]" placeholder="New List Title" id="list-title" class="form-control">
    <span class="input-group-btn">
      <input class="btn btn-primary" type="submit" value="Add New List">
    </span>
  </div>
</form>